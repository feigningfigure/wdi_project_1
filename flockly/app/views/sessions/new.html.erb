<h1>Login</h1>
<%= link_to "Sign Up", signup_path %>
  <p>
  <%= form_tag sessions_path do %>
  <%= label_tag :Email %><br>
  <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag 'Login' %></p>
<% end %>
